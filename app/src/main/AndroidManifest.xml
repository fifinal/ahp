            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.HasilActivity" />
        <activity android:name=".ui.InfoActivity" />
        <activity android:name=".ui.DiagnosaActivity" />
        <activity android:name=".ui.LandingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.admin.LoginActivity" />
        <activity android:name=".ui.MainActivity" />
